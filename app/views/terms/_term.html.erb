<%= turbo_frame_tag(dom_id(term)) do %>
  <div class="bg-sky-500 px-2 py-1 text-lg text-gray-50 rounded-lg">
    <%= link_to student_term_student_terminal_subjects_url(@student, term), data: {"turbo_frame": "_top"} do %>
      <%= term.name %>
      <div class="bg-sky-700 text-sm">
        <%= term.start_date.inspect %>
        to
        <%= term.end_date.inspect %>
      </div>
    <% end %>
    <div>
      <%= link_to edit_student_term_url(@student, term), class: "bg-sky-700 px-1 rounded  text-sm" do %>
        Edit
      <% end %>
    </div>
  </div>
<% end %>